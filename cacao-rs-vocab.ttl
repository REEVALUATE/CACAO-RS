cacao-rs:CC-BY-NC-ND_4.0 a odrl:Policy ;
    odrl:obligation [ odrl:action cc:Attribution,
                cc:Notice,
                odrl:attribute,
                odrl:inform ] ;
    odrl:permission [ odrl:action cc:Distribution,
                cc:Reproduction,
                cc:Sharing,
                odrl:archive,
                odrl:concurrentUse,
                odrl:delete,
                odrl:digitize,
                odrl:display,
                odrl:distribute,
                odrl:execute,
                odrl:extract,
                odrl:include,
                odrl:index,
                odrl:install,
                odrl:move,
                odrl:play,
                odrl:present,
                odrl:print,
                odrl:read,
                odrl:reproduce,
                odrl:stream,
                odrl:synchronize,
                odrl:textToSpeech,
                odrl:uninstall ] ;
    odrl:prohibition [ odrl:action odrl:ensureExclusivity,
                odrl:grantUse,
                odrl:nextPolicy,
                odrl:reviewPolicy,
                odrl:watermark ] .

